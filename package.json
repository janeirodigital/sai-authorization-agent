{
  "name": "sai-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:prod": "ng serve --host 127.0.0.1 --disable-host-check true --configuration=production",
    "start:dev": "ng serve --host 127.0.0.1 --disable-host-check true --configuration=development",
    "start:ssl": "ng serve --ssl",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && ng run sai-web-app:storybook",
    "build-storybook": "npm run docs:json && ng run sai-web-app:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.7",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.2.7",
    "@angular/compiler": "^14.2.7",
    "@angular/core": "^14.2.7",
    "@angular/forms": "^14.2.7",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "^14.2.7",
    "@angular/platform-browser-dynamic": "^14.2.7",
    "@angular/pwa": "14.2.6",
    "@angular/router": "^14.2.7",
    "@angular/service-worker": "^14.2.7",
    "@inrupt/solid-client-authn-browser": "^1.11.7",
    "@janeirodigital/sai-api-messages": "^0.6.0",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/entity": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@rdfjs/data-model": "^2.0.1",
    "ngrx-store-localstorage": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@babel/core": "^7.17.0",
    "@compodoc/compodoc": "^1.1.18",
    "@rdfjs/types": "^1.1.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/angular": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.2"
  },
  "volta": {
    "node": "16.17.0"
  }
}
